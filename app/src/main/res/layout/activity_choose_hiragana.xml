<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/dark_mode"
tools:context=".ChooseHiragana">

    <CheckBox
        android:id="@+id/h_KY"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="33dp"
        android:text="@string/h_ky"
        android:textColor="#00FFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/h_GY"
        app:layout_constraintEnd_toStartOf="@+id/h_NY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/h_N" />

<CheckBox
    android:id="@+id/h_PY"
    android:layout_width="0dp"
    android:layout_height="0dp"
