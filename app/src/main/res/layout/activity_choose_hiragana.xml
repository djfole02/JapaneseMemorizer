    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_KY"
    app:layout_constraintEnd_toStartOf="@+id/h_NO"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/h_R" />

<CheckBox
    android:id="@+id/h_M"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginStart="34dp"
    android:layout_marginEnd="33dp"
    android:text="@string/h_m"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_R"
    app:layout_constraintEnd_toStartOf="@+id/h_Y"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/h_H" />

<CheckBox
    android:id="@+id/h_Z"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginEnd="34dp"
    android:text="@string/h_z"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_D"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/h_K"
    app:layout_constraintTop_toBottomOf="@+id/h_G" />

<CheckBox
    android:id="@+id/h_Y"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginEnd="34dp"
    android:layout_marginBottom="1dp"
    android:text="@string/h_y"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_W"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/h_M"
    app:layout_constraintTop_toBottomOf="@+id/h_P" />

<CheckBox
    android:id="@+id/h_D"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginEnd="34dp"
    android:text="@string/h_d"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_B"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/h_S"
    app:layout_constraintTop_toBottomOf="@+id/h_Z" />

<CheckBox
    android:id="@+id/h_B"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginEnd="34dp"
    android:text="@string/h_b"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_P"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/h_T"
    app:layout_constraintTop_toBottomOf="@+id/h_D" />

<CheckBox
    android:id="@+id/h_P"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginEnd="34dp"
    android:text="@string/h_p"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_Y"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/h_H"
    app:layout_constraintTop_toBottomOf="@+id/h_B" />

<CheckBox
    android:id="@+id/h_G"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginTop="46dp"
    android:layout_marginEnd="34dp"
    android:text="@string/h_g"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_Z"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/h_"
    app:layout_constraintTop_toTopOf="parent" />

<CheckBox
    android:id="@+id/h_W"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginEnd="34dp"
    android:text="@string/h_w"
    android:textColor="#00FFFF"
    app:layout_constraintBottom_toTopOf="@+id/h_NO"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/h_R"
    app:layout_constraintTop_toBottomOf="@+id/h_Y" />

    <Button
        android:id="@+id/h_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="12dp"
        android:onClick="chosen"
        android:text="@string/continue_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/h_RY"
        app:layout_constraintTop_toBottomOf="@+id/h_PY" />

</androidx.constraintlayout.widget.ConstraintLayout>