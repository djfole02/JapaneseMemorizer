        app:layout_constraintBottom_toTopOf="@+id/h_T"
        app:layout_constraintEnd_toStartOf="@+id/h_D"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/h_K" />

    <CheckBox
        android:id="@+id/h_T"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="33dp"
        android:text="@string/h_t"
        android:textColor="#00FFFF"
        app:layout_constraintBottom_toTopOf="@+id/h_H"
        app:layout_constraintEnd_toStartOf="@+id/h_B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/h_S" />

